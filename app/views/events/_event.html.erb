<div id="<%= dom_id event %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Date:</strong>
    <%= event.date %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Location:</strong>
    <%= event.location %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Summary:</strong>
    <%= event.summary %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Contact:</strong>
    <%= event.contact.full_name %>
  </p>

</div>
